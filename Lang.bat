
@PROMPT $
@CD /D "%~DP0"



@SWREG QUERY "HKCU\Console_combofixbackup" >N_\%random% ||(
	@SWREG COPY "HKCU\Console" "HKCU\Console_combofixbackup" /s
	@SWREG ADD "HKCU\Console" /v "QuickEdit" /T REG_DWORD /D 0
	@SWREG ADD "HKCU\Console" /V "InsertMode" /T REG_DWORD /D 1
	)

@IF NOT EXIST CHCP.bat (
	SWREG QUERY "HKLM\SYSTEM\CurrentControlSet\Control\Nls\CodePage" /V ACP >NlsCodePageACP00
	IF EXIST %system%\chcp.com (
		SED "/.*	/!d; s//@CHCP.com /" NlsCodePageACP00 > CHCP.bat
		@ECHO.@>> CHCP.bat
	) ELSE SED "/.*	/!d; s//@Mode.com con codepage select=/" NlsCodePageACP00 >CHCP.bat
	DEL /A/F NlsCodePageACP00
	FOR /F "TOKENS=2" %%G IN ( CHCP.bat ) DO @(
		SWREG ADD HKCU\Console /V CodePage /T REG_DWORD /D "%%G"
		SWREG ADD HKU\S-1-5-18\Console /V CodePage /T REG_DWORD /D "%%G"
		) )
@CALL CHCP.bat

@IF NOT EXIST NlsLanguageDefault (
	SWREG QUERY HKLM\System\CurrentControlSet\Control\NLS\Language /V Default >NlsLanguage00
	SED "/.*	/!d; s///" NlsLanguage00 >NlsLanguageDefault
	DEL /A/F NlsLanguage00
	)

